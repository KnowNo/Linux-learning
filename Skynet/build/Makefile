  AR = ar
endif

ifeq ($(config),debug)
  OBJDIR     = obj/Debug
  TARGETDIR  = ../bin/debug
  TARGET     = $(TARGETDIR)/SkynetPro
  DEFINES   += -D_REENTRANT -D_GNU_SOURCE -DDEBUG
  INCLUDES  += -I../src
  CPPFLAGS  += -MMD -MP $(DEFINES) $(INCLUDES)
  CFLAGS    += $(CPPFLAGS) $(ARCH) -Wall -g `pkg-config --cflags glib-2.0`
  CXXFLAGS  += $(CFLAGS) 
  LDFLAGS   += -s -ldl -rdynamic -lm -lglib-2.0 -lpthread
  LIBS      += 
  RESFLAGS  += $(DEFINES) $(INCLUDES) 
  LDDEPS    += 
  LINKCMD    = $(CC) -o $(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(ARCH) $(LIBS)
  define PREBUILDCMDS
  endef
